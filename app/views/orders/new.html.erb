<div class="container mt-3">
  <%= form_with(model: @order, local: true) do |f| %>
    <%= render 'form', order: @order %>

    <h3>予約ページ</h3>
    <div class="field">
      <h4>診察券番号</h4>
      <%= f.text_field :user_id %>
      <h4>なまえ</h4>
      <%= f.text_field :name %>
    </div>

    <div class="actions">
      <%= f.submit "予約番号発行" %>
    </div>
  <% end %>


  <%= link_to 'Back', search_path %>
</div>